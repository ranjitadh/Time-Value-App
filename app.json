{
  "expo": {
    "name": "react_native",
    "slug": "react_native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "reactnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.react-native",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#123123"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.react_native"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#123123"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2f2a6880-bb28-4d8c-825c-c4be97e8bfdc"
      }
    }
  }
}
